{
	"name": "nfeui",
	"version": "0.0.1",
	"description": "牛卡福PC端UI组件库",
	"main": "dist/nfeui.umd.js",
	"module": "dist/nfeui.es.js",
	"typings": "dist/index.d.ts",
	"style": "dist/style.css",
	"author": "",
	"license": "MIT",
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"dev": "gc-vitepress dev --root=docs --host",
		"build": "npm run ui-build && npm run docs-build",
		"type": "vue-tsc --declaration --emitDeclarationOnly",
		"test": "jest",
		"add": "zx nucarf/demoTemplate/create/index.mjs ",
		"np": "np",
		"commit": "git-cz",
		"generate:types": "zx nucarf/generate-types.mjs ",
		"ui-build": "vite build && npm run type && npm run generate:types",
		"docs-build": "zx nucarf/docs-build.mjs",
		"docs-serve": "cross-env NODE_ENV=production gc-vitepress serve --root=docs",
		"docs-build-serve": "zx nucarf/docs-build-serve.mjs",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
		"lint:eslint": "eslint --cache --max-warnings 0  \"{packages,mock}/**/*.{vue,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write  \"packages/**/*.{js,json,tsx,css,less,scss,vue,html,md}\" "
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@ls-lint/ls-lint": "^1.9.2",
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@types/jest": "^26.0.15",
		"@types/node": "^17.0.13",
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.8.1",
		"@vitejs/plugin-vue": "^1.1.4",
		"@vitejs/plugin-vue-jsx": "^1.1.7",
		"@vue/compiler-sfc": "^3.2.29",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-typescript": "^7.0.0",
		"@vue/test-utils": "^2.0.0-beta.10",
		"@vuedx/typescript-plugin-vue": "^0.6.3",
		"art-template": "^4.13.2",
		"autoprefixer": "^10.4.2",
		"babel-generator": "^6.26.1",
		"babel-jest": "^27.4.6",
		"babel-traverse": "^6.26.0",
		"commitizen": "^4.2.4",
		"conventional-changelog-cli": "^2.1.1",
		"cross-env": "^7.0.2",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^14.3.0",
		"element-plus": "^1.3.0-beta.7",
		"eslint": "^7.13.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-vue": "^7.1.0",
		"generate-components-vitepress": "^1.0.5",
		"gh-pages": "^3.1.0",
		"globby": "^13.1.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"np": "^7.4.0",
		"postcss": "^8.4.5",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.66.1",
		"rollup-plugin-alias": "^2.2.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-css-only": "^2.1.0",
		"rollup-plugin-typescript2": "^0.30.0",
		"rollup-plugin-vue": "^6.0.0-beta.11",
		"stylelint": "^14.3.0",
		"stylelint-config-standard": "^24.0.0",
		"tailwindcss": "^3.0.17",
		"ts-jest": "^26.4.4",
		"typescript": "^4.4.4",
		"unplugin-auto-import": "^0.5.11",
		"unplugin-vue-components": "^0.17.14",
		"vite": "^2.7.13",
		"vue-jest": "^5.0.0-alpha.6",
		"vue-tsc": "^0.29.2",
		"zx": "^4.3.0"
	},
	"keywords": [
		"vue",
		"vue3",
		"components",
		"nfeui",
		"nucarf"
	],
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"registry": "http://npm.nucarf.cn/"
	},
	"np": {
		"yarn": false
	},
	"dependencies": {
		"unplugin-element-plus": "^0.2.0",
		"vue": "^3.2.29"
	},
	"lint-staged": {
		"*.md": "prettier --write",
		"*.{ts,tsx,js,vue,less}": "prettier --write",
		"*.{ts,tsx,js,vue}": "eslint --fix",
		"*.{vue,css,less}": "stylelint --fix"
	}
}
